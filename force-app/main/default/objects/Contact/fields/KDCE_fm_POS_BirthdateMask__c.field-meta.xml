<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>KDCE_fm_POS_BirthdateMask__c</fullName>
    <description>POS Birthdate Masking</description>
    <externalId>false</externalId>
    <formula>SUBSTITUTE( TEXT( Birthdate ), MID( TEXT( Birthdate ), 6, 5 ), &quot;**-**&quot; )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Birthdate(Mask) for POS</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
